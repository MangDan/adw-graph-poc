   formData: {\n        graph: \"G_NH_STOCK\",\n        limit: 10,\n        searchTerm: \"\"\n      },\n      icons: [{\n        text: \"organization-24\",\n        value: \"octicon:organization-24\"\n      }, {\n        text: \"organization\",\n        value: \"icons8:organization\"\n      }],\n      selectedLabel: null,\n      selectedIcon: null,\n      selectedTempIcon: \"bi:x-lg\",\n      selectedShape: null,\n      selectedSize: 0,\n      selectedColor: null,\n      showHexValue: null,\n      graphItems: [{\n        text: \"종목\",\n        value: \"G_NH_STOCK\"\n      }, {\n        text: \"제품\",\n        value: \"G_NH_PRODUCT\"\n      }, {\n        text: \"인물\",\n        value: \"G_NH_PEOPLE\"\n      }, {\n        text: \"테마\",\n        value: \"G_NH_THEME\"\n      }],\n      labelItems: [\"종목\", \"제품\", \"인물\", \"테마\", \"기관\", \"회사\", \"이벤트\"],\n      shapeItems: [\"square\", \"dot\", \"box\", \"triangel\", \"triangelDown\", \"ellipse\", \"diamond\", \"hexagon\", \"star\"],\n      isSelectLabel: true,\n      iconDialog: false,\n      colorDialog: false,\n      config: {\n        node: {\n          caption: [\"V_NAME\", \"AGENT_NAME\"],\n          defaultIcon: false,\n          icon: {\n            종목: \"ant-design:stock-outlined\",\n            제품: \"gg:product-hunt\",\n            인물: \"bi:person-fill\",\n            테마: \"arcticons:themes\",\n            기관: \"octicon:organization-24\",\n            회사: \"carbon:location-company\",\n            이벤트: \"gg:eventbrite\"\n          },\n          shape: {\n            종목: \"dot\",\n            제품: \"dot\",\n            인물: \"dot\",\n            테마: \"dot\",\n            기관: \"dot\",\n            회사: \"dot\",\n            이벤트: \"dot\"\n          },\n          size: {\n            종목: 20,\n            제품: 20,\n            인물: 20,\n            테마: 20,\n            기관: 20,\n            회사: 20,\n            이벤트: 20\n          },\n          color: {\n            종목: \"#4666af\",\n            제품: \"#af46a7\",\n          